<!-- move to correct folder -->
<script>
  document.addEventListener("DOMContentLoaded", function(event) {
    $("#menu-toggle").click(function(e) {
      e.preventDefault();
      $("#wrapper").toggleClass("toggled");
    });
  });
</script>
